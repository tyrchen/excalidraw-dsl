# Test edge connections at shape boundaries
start[Start Node] {
  backgroundColor: "#dcfce7";
}

process1[Process 1] {
  backgroundColor: "#dbeafe";
}

process2[Process 2] {
  backgroundColor: "#fef3c7";
}

decision[Decision?] {
  shape: "diamond";
  backgroundColor: "#f3e8ff";
}

end[End Node] {
  backgroundColor: "#fee2e2";
}

# Connections to test edge binding
start -> process1
process1 -> decision
decision -> process2{Yes}
decision -> end{No}
process2 -> end